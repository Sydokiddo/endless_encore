{
  "schemaVersion": 1,
  "id": "endlessencore",
  "version": "${version}",

  "name": "Endless Encore",
  "description": "A revamp of Minecraft's End dimension in its entirety, complete with new building blocks, biomes, mobs, items, and mechanics!",
  "authors": [
    "Sydokiddo (Lead Development)",
    "AmbientUnder (Sound Design + Additional Music/Textures)",
    "Dopadream (Additional Coding Help)",
    "LunarLuned (Additional Coding + Idea Help)",
    "Thaetaa-Terraainn (Music)"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/endless-encore",
    "sources": "https://github.com/Sydokiddo/endless_encore"
  },

  "license": "MIT",
  "icon": "assets/endlessencore/icon.png",
  "accessWidener" : "endlessencore.accesswidener",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.sydokiddo.endlessencore.EndlessEncore"
    ],
    "client": [
      "net.sydokiddo.endlessencore.client.EndlessEncoreClient"
    ],
    "modmenu": [
      "net.sydokiddo.endlessencore.config.ModMenuCompatibility"
    ]
  },
  "mixins": [
    "endlessencore.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "1.19.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}